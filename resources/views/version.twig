{% extends "base.twig" %}
{% block title %} Versión {% endblock %}
{% block head %}{{parent()}}
<link rel="stylesheet" type="text/css" href="./css/inicio.css?id={{random()}}">
<style type="text/css">
	#inicio {
		background: url('./images/fondos/0.jpg');
		background-position: center -40%;
	    background-size: cover;
	    width: 100%;
	    max-width: 100%;
	    max-height: 100%;
	    height: 100%;
	    background-repeat: no-repeat;
	    /*background-position: 0px 0px; */
	    background-attachment: fixed;
	}
	@media (min-width: 720px) {
		#inicio {
			background-position: 50% -32%;
		}    	
	}
	@media (min-width: 1440px) {
		#inicio {
			background-position: 50% -10%;
		}  
	}
</style>
{% endblock %}
{% block content %}
<section id="inicio">
	{% include 'includes/header.twig' %}
	<div class="content-cabecera">
		<div class="content">
			<p class="txt-frase" style="padding-top:30px; ">
			Ancaor Web -  Detalles de Versión
			</p>
			<p class="txt-about" style="">
				Version v0.0.0 - beta
			</p>
			<p class="txt-about" style="">
				- Arquitectura MVC <br>
			</p>
			<p class="txt-about" style="">
				Version v0.0.1 - beta
			</p>
			<p class="txt-about" style="">
				- Arquitectura MVC <br>
				- Implementación Ajax
			</p>
		</div>
	</div>
</section>

{% endblock %}